package introductiontojava_lms;

//print() and println() statements used from PrintStream class
public class Starpattern {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i=1;i<=5;i++)
		{
			for(int j=0;j<i;j++)
			{
				
				System.out.print("*");
				
			}
		    System.out.println();
		}
	}

}
